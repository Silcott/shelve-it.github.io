    <!-- for sortable ui -->
    <script>
    $(function() {
        $( "#sortable" ).sortable({
         containment: '#wrapper',
         revert: true }); 
        
        $('.show-abc').click(function(){
            
            if ( $(this).hasClass('hiding') ) {
                $(this).removeClass('hiding').addClass('showing');
                $(this).text('Hide Alphabet');    
            }
            else {
                $(this).removeClass('showing').addClass('hiding');
                $(this).text('Show Alphabet');    
            }
            
            $('#abc-wrapper').fadeToggle();
            return false;
        })
         
    });
    

    
    </script>
    
